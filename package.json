{
  "name": "<{{project.name}}>",
  "version": "1.0.0",
  "description": "<{{project.description}}>",
  "main": "src/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist",
    "zip": "cd dist/ && bestzip ../dist.zip * && cd ../",
    "deploy": "npm run build && npm run zip && aws lambda update-function-code --function-name <{{project.function.prod}}> --zip-file fileb://dist.zip",
    "deploy-prod": "npm run build && npm run zip && aws lambda update-function-code --function-name <{{project.function.prod}}> --zip-file fileb://dist.zip",
    "dev": "npm run build && sam local start-api --template sam.local.yaml",
    "dev-w": "npm watch 'npm run build' src -d 1000"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/node": "^18.11.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vercel/ncc": "^0.38.1",
    "bestzip": "^2.2.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "^5.3.3",
    "watch": "^1.0.2",
    "zip": "^1.2.0"
  },
  "dependencies": {
    "aws-sdk": "^2.1537.0",
    "mssql": "^10.0.2",
    "typeorm": "^0.3.19",
    "zod": "^3.22.4"
  }
}
